{
    "version": 2,
    "builds": [
        {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
            "includeFiles": [
            "views/**/*",
            "public/**/*"
            ]
        }
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "server/server.js"
        }
    ]
}